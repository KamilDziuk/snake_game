                <Button fx:id="btnUp" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Up" GridPane.columnIndex="1" />
                <Button fx:id="btnLeft" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="L" GridPane.rowIndex="1" />
                <Button fx:id="btnRight" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="R" GridPane.columnIndex="2" GridPane.rowIndex="1" />
